package com.Practic_Programm.Pack1;
import java.util.Scanner;

public class PrimeNo {

	public static void main(String[] args) {
		 
		
		int num;
		int count=0;
		
		
		for(;;)
		{
		System.out.println("Enter a No");
		Scanner sc = new Scanner(System.in);					//declare for user a space
		num = sc.nextInt();
		
		
		
			for(int i=1;i<=num;i++)
			{
				if(num%i==0)
					count++;
			}
			
		
		if(count ==2)
		{
			System.out.println("Prime");
		}
		else
		{
			System.out.println("Not a Prime");
			
		}
		}

	}

}
